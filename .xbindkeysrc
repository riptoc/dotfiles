# Play/Pause
"playerctl play-pause"
   XF86AudioPlay

# Previous
"playerctl previous"
   XF86AudioPrev

# Next
"playerctl next"
   XF86AudioNext

# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +10%"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -10%"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Increase backlight
"xbacklight -inc 20"
   XF86MonBrightnessUp

# Decrease backlight
"xbacklight -dec 20"
   XF86MonBrightnessDown
